{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.EntityFrameworkCore.Database.Command": "Error",
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "Flatly"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ApplicationDbContext": "Host=localhost;Port=5432;Database=flatly;User Id=postgres;Password=1;Timezone=Europe/Minsk;"
  },
  "ParsingOptions": {
    "Links": [
      "https://realt.by/rent/flat-for-long/"
    ],
    "ObjectUrlTemplate": "https://realt.by/rent-flat-for-long/object/{code}/",
    "PageConcurrency": 8,
    "DetailConcurrency": 32,
    "SaveBatchSize": 400,
    "ThrottleMinMs": 50,
    "ThrottleMaxMs": 150,
    "SkipUnchanged": true
  },
  "Keycloak": {
    "AuthorizationUrl": "http://localhost:8080/realms/flatly/protocol/openid-connect/auth"
  },
  "Authorization": {
    "MetadataAddress": "http://localhost:8080/realms/flatly/.well-known/openid-configuration",
    "ValidateIssuer": "http://localhost:8080/realms/flatly",
    "Audience": "account"
  },
  "JwtOptions": {
    "SecretKey": "secretSecretSecretSecretSecretSecretSecret",
    "AccessTokenExpirationMinutes": 15,
    "RefreshTokenExpirationDays": 7
  }
}